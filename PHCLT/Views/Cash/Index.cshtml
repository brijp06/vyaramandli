<h2>કેશ કલેક્સન</h2>
<div class="card card-default">
    <input type="hidden" id="hdnCmbTalukaId" value="@ViewBag.talikaid" />
    <div class="card-header">
        <h3 class="card-title">કેશ કલેક્સન</h3>

        <div class="card-tools">
            <button type="button" class="btn btn-tool" data-card-widget="collapse">
                <i class="fas fa-minus"></i>
            </button>
        </div>
    </div>
    <!-- /.card-header -->
    <div class="card-body">
        <div class="row">
            <div class="col-md-6">
                <div class="form-group">
                    <label for="exampleInputEmail1">બીલ નંબર:-</label>
                    <input type="text" class="form-control" id="billno" placeholder="" value="@ViewBag.billno" disabled="disabled">
                </div>
            </div>
            <!-- /.col -->
            <div class="col-md-6">
                <div class="form-group">
                    <label for="exampleInputEmail1">તારીખ:-</label>
                    <input type="date" class="form-control" id="txtEntryDate" placeholder="Select Entry Date">
                </div>
            </div>

            <div class="col-md-6">
                <div class="form-group">
                    <label for="exampleInputEmail1">નામ:-</label>
                    @*<select class="form-control select2bs4" name="usermaster" id="usermaster" style="width: 100%;">
            <option selected="selected">Select a Member</option>

            @foreach (var distMaster in ViewBag.usermaster)
            {
                //<option value="@distMaster.Id">@distMaster.DisName</option>
                <option value="@distMaster.Id" @(ViewBag.SelectedDistrictId == distMaster.Id ? "selected" : "")>@distMaster.username</option>
            }
        </select>*@
                    <select class="form-control select2bs4" name="usermaster" id="usermaster" style="width: 100%;">
                        <option selected="selected" value="">Select a Member</option>
                        @foreach (var distMaster in ViewBag.usermaster)
                        {
                            <option value="@distMaster.Id" @(ViewBag.SelectedDistrictId == distMaster.Id ? "selected" : "")>
                                @distMaster.username
                            </option>
                        }
                    </select>
                    <span id="errorMsg" style="color:red; display:none;">કૃપા કરી નામ Select કરો.</span>

                </div>
            </div>
            <div class="col-md-6">
                <div class="form-group">
                    <div>
                        <label for="exampleInputEmail1" style="color:blue">વેચાણ રકમ:- @ViewBag.saleamt</label>
                    </div>
                    <div>
                        <label for="exampleInputEmail1" style="color:red">જમા કરાવેલ રકમ:- @ViewBag.receiptamt</label>
                    </div>
                    <div>
                        <label for="exampleInputEmail1" style="color:blue">અન્ય રકમ:- @ViewBag.extraamt</label>
                    </div>
                    <div>
                        <label for="exampleInputEmail1" id="lblTotal" style="color:blueviolet">બાકી રકમ:- @ViewBag.totalamt</label>
                    </div>
                </div>
            </div>

            <div class="col-md-6">
                <div class="form-group">
                    <label for="exampleInputEmail1">ભરવા પાત્ર રકમ:-</label>
                    <input type="text" class="form-control" id="Qty" placeholder="Enter Amount" value="@ViewBag.Qty">
                    <span id="errorMsg" style="color:red; display:none;">બાકી રકમ થી વધારે તમે જમા થશે નહિ.</span>
                </div>
            </div>
        </div>
    </div>
    <div class="card-footer">
        <button type="button" id="btnsave" class="btn btn-primary">Save</button>
        @*<button type="button" id="btnviewlist" class="btn btn-primary">View List</button>*@
    </div>
</div>
<script src="~/plugins/daterangepicker/daterangepicker.js"></script>

<script>
    $(function () {
        var today = new Date();

        // Format today's date as yyyy-mm-dd
        var formattedDate = today.toISOString().substr(0, 10);


        // Set the value of the date picker input field
        $('#txtEntryDate').val(formattedDate);
    })

</script>


